{
	"name": "SinkAllTablesDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"TableName": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().FileName,formatDatetime(utcnow(),'-dd-MM-yyy hh:mm:ss'),'.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().FileName,'/',formatDatetime(utcnow(),'yyyy'),'/',formatDatetime(utcnow(),'MM'),'/',formatDatetime(utcnow(),'dd'),'/')\n",
					"type": "Expression"
				},
				"container": "loan-container"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}